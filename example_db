CREATE DATABASE SuperProfDB;

USE SuperProfDB;

CREATE TABLE `USER` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `name` VARCHAR(45) NULL,
  `surname` VARCHAR(45) NULL,
  `birth` DATE NULL,
  `gender` ENUM('M','F') NULL,
  `email` VARCHAR(45) NULL,
  PRIMARY KEY (`id`))
ENGINE = InnoDB;

CREATE TABLE  `PROF ` (
  `user_id` INT,
  `id` INT NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`id`)
  FOREIGN KEY (`user_id`) REFERENCES `USER`(`id`))
ENGINE = InnoDB;  
